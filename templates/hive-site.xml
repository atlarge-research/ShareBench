<!--{{templates.disclaimer}}-->
<configuration>
    <property>
        <name>fs.s3a.impl</name>
        <value>org.apache.hadoop.fs.s3a.S3AFileSystem</value>
    </property>
    <property>
        <name>fs.s3a.endpoint</name>
        <value>http://{{services.ip}}:{{services.minio.ports.core}}</value>
    </property>
    <property>
        <name>fs.s3a.path.style.access</name>
        <value>true</value>
    </property>
   <property>
      <name>fs.s3a.access.key</name>
      <value>{{services.minio.access_key}}</value>
   </property>
   <property>
      <name>fs.s3a.secret.key</name>
      <value>{{services.minio.secret_key}}</value>
   </property>
    <property>
        <name>fs.s3a.awsAccessKeyId</name>
        <value>{{services.minio.access_key}}</value>
    </property>
    <property>
        <name>fs.s3a.awsSecretAccessKey</name>
        <value>{{services.minio.secret_key}}</value>
    </property>
    <property>
        <name>fs.s3a.connection.ssl.enabled</name>
        <value>false</value>
    </property>
</configuration>
